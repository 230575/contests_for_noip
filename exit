[32m+[m[32m    while(cnt<ans) {[m
[32m+[m[32m        if(!f[now]&&b[now]==a[now]) {[m
[32m+[m[32m            f[now]=1;[m
[32m+[m[32m        } else if(b[now]<a[now]) {[m
[32m+[m[32m            printf("P");[m
[32m+[m[32m            b[now]++;[m
[32m+[m[32m            cnt++;[m
[32m+[m[32m        };[m
[32m+[m[32m        if(now==1&&tmp==-1) {[m
[32m+[m[32m            tmp=1;[m
[32m+[m[32m        } else if(now==n&&tmp==1) {[m
[32m+[m[32m            tmp=-1;[m
[32m+[m[32m        }[m
[32m+[m[32m        if(tmp==1) {[m
[32m+[m[32m            printf("R");[m
[32m+[m[32m            now++;[m
[32m+[m[32m        } else {[m
[32m+[m[32m            printf("L");[m
[32m+[m[32m            now--;[m
[32m+[m[32m        }[m
[32m+[m[32m    }[m
[32m+[m[32m    return 0;[m
[32m+[m[32m}[m
