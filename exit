[32m+[m[32m#include <bits/stdc++.h>[m
[32m+[m[32musing namespace std;[m
[32m+[m
[32m+[m[32mint n,f[1000010][20],dep[1000010],tot,ans=2,est;[m
[32m+[m
[32m+[m[32mint lca(int x,int y) {[m
[32m+[m[32m    if(dep[x]<dep[y]) swap(x,y);[m
[32m+[m[32m    int tmp=dep[x]-dep[y];[m
[32m+[m[32m    for(int i=0;(1<<i)<=tmp;i++) {[m
[32m+[m[32m        if((1<<i)&tmp) x=f[x][i];[m
[32m+[m[32m    }[m
[32m+[m[32m    if(x==y) return x;[m
[32m+[m[32m    for(int i=20;i>=0;i--) {[m
[32m+[m[32m        if((1<<i)<=dep[x]&&f[x][i]!=f[y][i]) x=f[x][i],y=f[y][i];[m
[32m+[m[32m    }[m
[32m+[m[32m    return f[x][0];[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mint main() {[m
[32m+[m[32m    scanf("%d",&n);[m
[32m+[m[32m    f[1][0]=1;[m
[32m+[m[32m    f[2][0]=f[3][0]=f[4][0]=1;[m
[32m+[m[32m    dep[1]=1;[m
[32m+[m[32m    dep[2]=dep[3]=dep[4]=2;[m
[32m+[m[32m    tot=4;[m
[32m+[m[32m    est=2;[m
[32m+[m[32m    for(int q,i=1;i<=n;i++) {[m
[32m+[m[32m        scanf("%d",&q);[m
[32m+[m[32m        tot++;[m
[32m+[m[32m        dep[tot]=dep[q]+1;[m
[32m+[m[32m        f[tot][0]=q;[m
[32m+[m[32m        for(int j=1;(1<<j)<=dep[tot];j++) {[m
[32m+[m[32m            f[tot][j]=f[f[tot][j-1]][j-1];[m
[32m+[m[32m        }[m
[32m+[m[32m        tot++;[m
[32m+[m[32m        dep[tot]=dep[q]+1;[m
[32m+[m[32m        f[tot][0]=q;[m
[32m+[m[32m        for(int j=1;(1<<j)<=dep[tot];j++) {[m
[32m+[m[32m            f[tot][j]=f[f[tot][j-1]][j-1];[m
[32m+[m[32m        }[m
[32m+[m[32m        if(dep[tot]>dep[est]) {[m
[32m+[m[32m            est=tot;[m
[32m+[m[32m            ans++;[m
[32m+[m[32m        } else {[m
[32m+[m[32m            int dis=dep[q]+dep[est]-2*dep[lca(q,est)]+1;[m
[32m+[m[32m            if(dis>ans) ans=dis;[m
[32m+[m[32m        }[m
[32m+[m[32m        cout<<ans<<endl;[m
[32m+[m[32m    }[m
[32m+[m[32m    return 0;[m
[32m+[m[32m}[m
\ No newline at end of file[m
